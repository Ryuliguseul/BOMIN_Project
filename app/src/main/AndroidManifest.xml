<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bomin_project">
    <uses-permission android:name="android.permission.INTERNET"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        android:networkSecurityConfig="@xml/network_security_config"

        >
        <uses-library android:name="org.apache.http.legacy" android:required="false" />
        <activity android:name=".Insurance"></activity>
        <activity android:name=".MainActivity"></activity>

        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity"></activity>

    <activity android:name=".ProductDetail"></activity>
        <activity android:name=".ShowTerms"></activity>
    <activity android:name=".Payment"></activity>
    <activity android:name=".TermsConditions"></activity>
        <activity android:name=".Success"></activity>
        <activity android:name=".Mypage"></activity>
        <activity android:name=".MySafetyScore"></activity>
        <activity android:name=".Mypoint"></activity>
        <activity android:name=".Accident"></activity>
        <activity android:name=".Myinsure"></activity>
    </application>

</manifest>